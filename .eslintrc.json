{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "impliedStrict": true,
      "experimentalObjectRestSpread": true,
      "arrowFunctions": true,
      "destructuring": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "import/no-unresolved": [
      2,
      {
        "caseSensitive": false
      }
    ],
    "react/prop-types": [0],
    "no-restricted-globals": [0],
    "no-confusing-arrow": [0],
    "implicit-arrow-linebreak": [0],
    "function-paren-newline": [0]
  }
}
